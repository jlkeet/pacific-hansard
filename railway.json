{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.railway-web"
  },
  "deploy": {
    "numReplicas": 1,
    "healthcheckPath": "/health.php",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "services": [
    {
      "name": "web",
      "source": {
        "repo": "https://github.com/jlkeet/pacific-hansard.git"
      }
    }
  ]
}